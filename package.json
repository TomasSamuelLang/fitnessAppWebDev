{
  "name": "fitnessapp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node tools/copyAssets",
    "build": "npm-run-all clean tsc sass-main sass copy-assets",
    "tsc": "tsc",
    "sass-main-watch": "sass --watch src/public/stylesheets/_main.scss:dist/public/css/main.css --style compressed",
    "sass-watch": "sass --watch src/public/stylesheets:dist/public/css --style compressed",
    "sass-main": "sass src/public/stylesheets/_main.scss:dist/public/css/main --style compressed",
    "sass": "sass src/public/stylesheets:dist/public/css --style compressed",
    "ejs-watch": "nodemon --watch views -e ejs --exec npm rum copy-assets",
    "tsc-watch": "nodemon --watch src -e ts --exec npm-run-all tsc start",
    "dev": "npm run clean & npm-run-all --parallel sass-main-watch sass-watch ejs-watch tsc-watch",
    "start": "node dist/bin/www.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/connect-flash": "0.0.35",
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/ejs": "^3.0.4",
    "@types/express": "^4.17.8",
    "@types/http-errors": "^1.8.0",
    "@types/mongoose": "^5.7.36",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.11.1",
    "@types/passport": "^1.0.4",
    "@types/passport-local": "^1.0.33",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "express": "^4.16.4",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongoose": "^5.10.6",
    "morgan": "~1.9.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "sass": "^1.26.11",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/fs-extra": "^9.0.1",
    "@types/shelljs": "^0.8.8",
    "fs-extra": "^9.0.1",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-node": "^9.0.0"
  }
}
